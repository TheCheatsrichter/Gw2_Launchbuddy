                            <Separator Margin="0,5,0,0"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="50*"/>
                                    <ColumnDefinition Width="50*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="32"/>
                                </Grid.RowDefinitions>
                                <Label Content="Background Color:" FontWeight="Bold"/>
                                <xctk:ColorPicker Name="ClrPcker_Background" SelectedColorChanged="ClrPcker_Background_SelectedColorChanged" Grid.Column="1" Margin="5"/>
                            </Grid>
                        </StackPanel>
                        <Separator Margin="0,10"/>
                        <StackPanel x:Name="Videomode" Visibility="Collapsed">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="50*"/>
                                    <ColumnDefinition Width="50*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="200"/>
                                    <RowDefinition Height="40"/>
                                </Grid.RowDefinitions>

                                <StackPanel>
                                    <Label Content="Videomode Info:" FontSize="16" FontWeight="Bold"/>
                                    <Label x:Name="lab_videoname" Content="Videoname:"/>
                                    <Label x:Name="lab_videopath" Content="Path:"/>
                                    <Label x:Name="lab_videoresolution" Content="Resolution:"/>
                                    <Label x:Name="lab_videoformat" Content="Format:"/>
                                    <Label x:Name="lab_videolength" Content="Length:"/>
                                </StackPanel>
                                <MediaElement x:Name="cinema_videoplayback" Grid.Row="0" Grid.Column="1" Margin="5,0" LoadedBehavior="Manual" UnloadedBehavior="Manual" Loaded="cinema_videoplayback_Loaded" MediaOpened="cinema_videoplayback_MediaOpened"/>

                                <Grid Grid.Row="1" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="50*"/>
                                        <ColumnDefinition Width="50*"/>
                                    </Grid.ColumnDefinitions>
                                    <Button x:Name="bt_cinema_videoplay" Content="&gt;" Grid.Row="0" Grid.Column="0" Margin="5" Click="bt_cinema_videoplay_Click"/>
                                    <Button x:Name="bt_cinema_videostop" Content="Stop" Grid.Row="0" Grid.Column="1" BorderThickness="5" Margin="5" Click="bt_cinema_videostop_Click"/>
                                </Grid>
                                <Button x:Name="bt_cinema_setvideo" Content="Set Video" Grid.Row="1" Margin="5" Click="bt_cinema_setvideo_Click"/>
                            </Grid>

                        </StackPanel>



                        <ScrollViewer Height="350" Background="#19000000" >
                            <StackPanel x:Name="Slideshow">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="50*"/>
                                        <ColumnDefinition Width="50*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="110"/>
                                        <RowDefinition Height="30"/>
                                    </Grid.RowDefinitions>
                                    <Label Content="Slideshow Info:" FontSize="16" FontWeight="Bold"/>

                                    <Image x:Name="img_imagepreview" Grid.Row="1" Grid.Column="1" Source="Resources/noimg.png"/>
                                    <Grid Margin="0,5" Grid.Row="1">
                                        <ListView x:Name="lv_cinema_images" Grid.Row="1" SelectionChanged="lv_cinema_images_SelectionChanged">
                                            <ListView.View>
                                                <GridView>
                                                    <GridViewColumn HeaderStringFormat="" Width="150" DisplayMemberBinding="{Binding Name}">
                                                        <GridViewColumnHeader Tag="Name" Click="listview_auth_Click" Content="Image Name"/>
                                                    </GridViewColumn>
                                                    <GridViewColumn HeaderStringFormat=""  DisplayMemberBinding="{Binding Path}">
                                                        <GridViewColumnHeader Tag="Path" Click="listview_auth_Click" Content="Path"/>
                                                    </GridViewColumn>
                                                </GridView>
                                            </ListView.View>
                                        </ListView>
                                    </Grid>
                                    <Button Content="Set Imagefolder" x:Name="bt_cinema_setimagefolder" Grid.Row="2" Grid.Column="0" Click="bt_cinema_setimagefolder_Click" />

                                    <Label x:Name="lab_imagepreview" Content="Current Image: -" Grid.Row="2" Grid.Column="1" FontWeight="Bold"/>
                                </Grid>
                                <Separator Margin="0,5,0,0"/>
